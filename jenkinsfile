
pipeline { 
    agent any
    stages {
        stage('push image') {
            steps {
                withAWS(credentials: 'aws_cren', region: 'us-east-1') {
                    sh 'docker build -t 699819973233.dkr.ecr.us-east-1.amazonaws.com/sprint:1 .'
                    sh 'docker push 699819973233.dkr.ecr.us-east-1.amazonaws.com/sprint:1'
                }
            }
        }
    }
}















